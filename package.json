{
  "name": "vue-tree-view",
  "private": true,
  "version": "0.0.1",
  "description": "Tree view headless component for Vue 3",
  "type": "module",
  "main": "./dist/tree-view.umd.js",
  "module": "./dist/tree-view.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/tree-view.esm.js",
      "require": "./dist/tree-view.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "vue",
    "vue3",
    "component",
    "library",
    "tree",
    " tree view"
  ],
  "author": "Daniel Dresner",
  "scripts": {
    "dev": "vite && vue-tsc --watch --noEmit",
    "build": "vue-tsc --noEmit && vite build",
    "lint": "eslint --ext .ts,vue --max-warnings=0 --ignore-path .gitignore .",
    "test": "vitest --coverage",
    "preview": "vite preview",
    "prepare": "husky install",
    "format": "prettier --write"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@types/jest": "29.5.12",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "@vitejs/plugin-vue": "4.5.2",
    "@vitest/coverage-v8": "1.2.2",
    "@vue/eslint-config-typescript": "12.0.0",
    "eslint": "8.56.0",
    "eslint-plugin-vue": "9.21.1",
    "husky": "9.0.10",
    "lint-staged": "15.2.2",
    "prettier": "3.2.5",
    "vite": "5.1.1",
    "vite-plugin-checker": "0.6.4",
    "vite-plugin-dts": "3.7.2",
    "vite-plugin-eslint": "1.8.1",
    "vitest": "1.2.2",
    "vue": "3.3.11",
    "vue-tsc": "1.8.27"
  },
  "lint-staged": {
    "*.{ts,vue}": "npm run lint",
    "*.{ts,vue,css,html,json,js,md}": "npm run format"
  }
}
